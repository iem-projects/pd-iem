#!/usr/bin/make
# -*- mode: Makefile -*-
# ############################### #
# common vars for building iemlib #
# ############################### #

all: clean depends build
.PHONY: all depends build install clean

REV=17459
SVNURL=https://svn.code.sf.net/p/pure-data/svn/trunk/externals/iemlib@$(REV)

depends::
	rm -rf $(LIBRARY)
	svn export -q $(SVNURL) $(LIBRARY)

clean::
	rm -rf $(LIBRARY)
